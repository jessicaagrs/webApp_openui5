<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog		
		title="{i18n>Commom.SelectAContract}"
		search="handleSearchChooseFromModal"
		confirm="handleCloseChooseFromModalSelection"
		cancel="handleCloseChooseFromModalSelection"
		items="{
			path: '/'
		}" >
		<StandardListItem
			title="{Title}"
			description="{Description}"			
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>
